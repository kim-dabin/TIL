  <mapper namespace="groups">
 	<select id="selectList" resultType="vo.Group">
 		select g.no, g.name, g.agent, g.debut_date debutDate, g.profile , g.regdate
 		from groups g
 	</select>
 	
 	<insert id="insert">
 		insert into groups(name, agent, debut_date, profile)
 		values()
 	
 	
 	</insert>
 	
  </mapper>